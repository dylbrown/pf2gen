<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<TabPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.controllers.equip.EquipTabController">
    <Tab closable="false" text="Purchase">
        <fx:include source="purchaseTab.fxml" fx:id="purchaseTab" />
    </Tab>
    <Tab closable="false" text="Enchant">
        <fx:include source="enchantTab.fxml" />
    </Tab>
    <Tab closable="false" text="Equip">
        <AnchorPane minHeight="0.0" minWidth="0.0">
            <SplitPane dividerPositions="0.5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <AnchorPane minHeight="0.0" minWidth="0.0">
                    <ListView fx:id="unequipped" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                </AnchorPane>
                <AnchorPane minHeight="0.0" minWidth="0.0">
                    <TableView fx:id="equipped" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <columns>
                            <TableColumn fx:id="nameCol" editable="false" text="Item">
                            </TableColumn>
                            <TableColumn fx:id="slotCol" editable="false" text="Slot">
                            </TableColumn>
                            <TableColumn fx:id="weightCol" editable="false" text="Weight">
                            </TableColumn>
                            <TableColumn fx:id="quantityCol" editable="false" text="Quantity">
                            </TableColumn>
                        </columns>
                  <columnResizePolicy>
                     <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                  </columnResizePolicy>
                    </TableView>
                </AnchorPane>
            </SplitPane>
        </AnchorPane>
    </Tab>
    <Tab closable="false" text="Formulas">
        <fx:include source="formulaTab.fxml"/>
    </Tab>
</TabPane>
